{
    "name": "acquia/blt-project",
    "description": "Project template for Drupal 8 sites built with BLT.",
    "type": "project",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "blt-drupal-check": {
            "type": "vcs",
            "url": "https://github.com/mikemadison13/blt-drupal-check.git",
            "no-api": true
        },
        "jquery-ui-touch-punch": {
            "type": "package",
            "package": {
                "name": "jquery-ui/jquery-ui-touch-punch",
                "version": "dev-master",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "jquery-ui-touch-punch"
                },
                "dist": {
                    "url": "https://api.github.com/repos/furf/jquery-ui-touch-punch/zipball",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        "superfish": {
            "type": "package",
            "package": {
                "name": "superfish/superfish",
                "version": "1.7.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "superfish"
                },
                "dist": {
                    "url": "https://api.github.com/repos/joeldbirch/superfish/zipball",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        "mmenu": {
            "type": "package",
            "package": {
                "name": "mmenu/mmenu",
                "version": "8.5.17",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "mmenu"
                },
                "dist": {
                    "url": "https://api.github.com/repos/FrDH/mmenu-js/zipball",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        "chartjs": {
            "type": "package",
            "package": {
                "name": "chartjs/chartjs",
                "version": "3.9.1",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "chartjs"
                },
                "dist": {
                    "url": "https://api.github.com/repos/chartjs/Chart.js/zipball",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        "slick": {
            "type": "package",
            "package": {
                "name": "slick/slick",
                "version": "v1.9.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/master.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        "dropzone": {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "5.7.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/enyo/dropzone/archive/v5.7.1.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "acquia/blt": true,
            "oomphinc/composer-installers-extender": true,
            "acquia/blt-phpcs": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-dom": "*",
        "ext-pdo": "*",
        "acquia/blt": "^13",
        "acquia/blt-behat": "dev-master",
        "acquia/blt-phpcs": "^1",
        "acquia/memcache-settings": "^1.1",
        "bower-asset/blazy": "^1.8",
        "chartjs/chartjs": "3.9.1",
        "cweagans/composer-patches": "^1.7",
        "drupal/acquia_connector": "^4",
        "drupal/acquia_purge": "^1.1",
        "drupal/admin_toolbar": "^3.3",
        "drupal/antibot": "^2.0",
        "drupal/autosave_form": "^1.4",
        "drupal/better_exposed_filters": "^6",
        "drupal/bg_image_formatter": "^1.16",
        "drupal/blazy": "^2.14",
        "drupal/block_exclude_pages": "^2",
        "drupal/captcha": "^1.1",
        "drupal/cloudflare": "2.0.0-alpha1",
        "drupal/components": "^3",
        "drupal/config_ignore": "2.x-dev",
        "drupal/config_split": "^2.0.0-rc4",
        "drupal/conflict": "^2.0@alpha",
        "drupal/contact_storage": "^1.3",
        "drupal/core": "^10",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core_context": "^1.0@beta",
        "drupal/ctools_block": "^4.0",
        "drupal/default_content": "^2",
        "drupal/devel": "^5.0.0",
        "drupal/diff": "^1.1",
        "drupal/dropzonejs": "^2.7",
        "drupal/embed": "^1.6",
        "drupal/entity_browser": "^2.9",
        "drupal/environment_indicator": "^4",
        "drupal/extlink": "^1.0",
        "drupal/field_group": "^3.0",
        "drupal/field_permissions": "^1.0",
        "drupal/flag": "^4.0-beta2",
        "drupal/focal_point": "^2",
        "drupal/google_analytics": "^4",
        "drupal/honeypot": "^2",
        "drupal/image_widget_crop": "^2.4",
        "drupal/inline_entity_form": "^1.0-rc7",
        "drupal/jquery_ui_droppable": "^2",
        "drupal/layout_builder_restrictions": "^2.17",
        "drupal/layout_builder_styles": "^2.0",
        "drupal/layout_library": "^1.0@beta",
        "drupal/linkicon": "1.x-dev",
        "drupal/media_entity_instagram": "^3.0",
        "drupal/media_entity_twitter": "^2.9",
        "drupal/metatag": "^2",
        "drupal/moderation_dashboard": "^2.1",
        "drupal/moderation_sidebar": "^1.7",
        "drupal/page_manager": "^4.0@RC",
        "drupal/panels": "^4.7",
        "drupal/panels_ipe": "^4.7",
        "drupal/pathauto": "^1.11",
        "drupal/purge": "^3.0.0",
        "drupal/qa_accounts": "1.x-dev",
        "drupal/r4032login": "^2",
        "drupal/recaptcha": "^3",
        "drupal/redirect": "^1.0",
        "drupal/responsive_menu": "4.4.x-dev",
        "drupal/search_api": "^1.17",
        "drupal/seckit": "^2",
        "drupal/shield": "^1.1",
        "drupal/simple_sitemap": "^4",
        "drupal/slick": "^2.7",
        "drupal/slick_entityreference": "^2.1",
        "drupal/smart_trim": "^2",
        "drupal/svg_image": "^3",
        "drupal/upgrade_status": "^4",
        "drupal/user_email_verification": "^1.2",
        "drupal/views_conditional": "^1.5",
        "drupal/webform": "^6.2.0-beta5",
        "drupal/webform_views": "^5.0-alpha8",
        "drush/drush": "^11",
        "enyo/dropzone": "^5",
        "guzzlehttp/promises": "1.5.0",
        "jquery-ui/jquery-ui-touch-punch": "dev-master",
        "laminas/laminas-zendframework-bridge": "^1.4",
        "mikemadison13/blt-drupal-check": "dev-main",
        "mikemadison13/blt-lando": "dev-main",
        "mmenu/mmenu": "^8.5",
        "slick/slick": "^1.9.0",
        "superfish/superfish": "^1.7"
    },
    "scripts": {
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ]
    },
    "extra": {
        "blt": {
            "update": true
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush",
                "type:drupal-drush",
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/core": {
                "2869592 - Disabled update module shouldn't produce a status report warning": "https://www.drupal.org/files/issues/2020-02-07/2869592-remove-update-warning-34.patch"
            },
            "acquia/blt": {
                "fixes zsh git hooks": "https://github.com/acquia/blt/pull/4497.patch",
                "debugging": "patches/debug.patch"
            },
            "mikemadison13/blt-lando": {
                "m1": "https://github.com/mikemadison13/blt-lando/pull/32.patch"
            },
            "drupal/google_analytics": {
                "install from config": "https://git.drupalcode.org/project/google_analytics/-/merge_requests/33.patch"
            }
        },
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            },
           "locations": {
                "web-root": "docroot/"
            },
            "file-mapping": {
                "[web-root]/.htaccess": false,
                "[web-root]/robots.txt": false,
                "[web-root]/sites/development.services.yml": false
            }
        },
        "enable-patching": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches-ignore": {
            "drupal/lightning_core": {
                "drupal/core": {
                    "2869592 - Disabled update module shouldn't produce a status report warning": "https://www.drupal.org/files/issues/2869592-remove-update-warning-7.patch"
                }
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Example\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    }
}
