{
    "name": "acquia/blt-project",
    "description": "Project template for Drupal 8 sites built with BLT.",
    "type": "project",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "superfish": {
            "type": "package",
            "package": {
                "name": "superfish/superfish",
                "version": "1.7.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "superfish"
                },
                "dist": {
                    "url": "https://api.github.com/repos/joeldbirch/superfish/zipball",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        "mmenu": {
            "type": "package",
            "package": {
                "name": "mmenu/mmenu",
                "version": "8.5.17",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "mmenu"
                },
                "dist": {
                    "url": "https://api.github.com/repos/FrDH/mmenu-js/zipball",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "acquia/blt": "^11.0",
        "acquia/lightning": "^4",
        "acquia/memcache-settings": "^1",
        "bower-asset/blazy": "^1.8",
        "drupal/acquia_connector": "^2",
        "drupal/acquia_purge": "^1.0",
        "drupal/adminimal_admin_toolbar": "^1.0",
        "drupal/better_exposed_filters": "^5.0-beta1",
        "drupal/block_exclude_pages": "^2",
        "drupal/captcha": "^1.1",
        "drupal/cloudflare": "^1.0.0-alpha7",
        "drupal/cog": "^1.10",
        "drupal/components": "^2.0-beta3",
        "drupal/config_ignore": "^2.1",
        "drupal/config_split": "^1.0.0",
        "drupal/core": "~8.9.0",
        "drupal/default_content": "^2",
        "drupal/devel": "^4.0.0",
        "drupal/environment_indicator": "^4",
        "drupal/extlink": "^1.0",
        "drupal/field_group": "^3.0",
        "drupal/field_permissions": "^1.0",
        "drupal/flag": "^4.0-beta2",
        "drupal/focal_point": "^1.5",
        "drupal/google_analytics": "^3",
        "drupal/honeypot": "^2",
        "drupal/linkicon": "1.x-dev",
        "drupal/panelizer": "^5",
        "drupal/purge": "^3.0.0-beta6",
        "drupal/qa_accounts": "1.x-dev",
        "drupal/r4032login": "dev-1.x",
        "drupal/recaptcha": "^3",
        "drupal/redirect": "^1.0",
        "drupal/responsive_menu": "^4.4",
        "drupal/seckit": "^2",
        "drupal/shield": "^1.1",
        "drupal/simple_sitemap": "^3.7",
        "drupal/smart_trim": "^1.0",
        "drupal/svg_image": "^1.8",
        "drupal/twig_xdebug": "^1.2",
        "drupal/upgrade_status": "^2.9",
        "drupal/webform": "^6",
        "drupal/webform_views": "^5.0-alpha8",
        "mmenu/mmenu": "^8.5",
        "superfish/superfish": "^1.7"
    },
    "require-dev": {
        "acquia/blt-require-dev": "11.x-dev",
        "drupal/console": "^1.0.0-rc13"
    },
    "scripts": {
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ]
    },
    "extra": {
        "blt": {
            "update": true
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush",
                "type:drupal-drush",
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/core": {
                "Ignore front end vendor folders to improve directory search performance": "https://www.drupal.org/files/issues/ignore_front_end_vendor-2329453-116.patch"
            },
            "drupal/responsive_menu": {
                "fixes php warning": "https://www.drupal.org/files/issues/2020-09-10/test_with_fix-3168579-23.patch"
            },
            "drupal/r4032login": {
                "d9 compatibility": "https://www.drupal.org/files/issues/2020-06-04/3042799-22_0.patch"
            }
        },
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            },
            "excludes": [
                "sites/development.services.yml",
                ".htaccess",
                "robots.txt"
            ]
        },
        "enable-patching": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        }
    },
    "autoload": {
        "psr-4": {
            "Example\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    }
}
